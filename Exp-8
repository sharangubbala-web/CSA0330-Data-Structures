#include <stdio.h>

int main() {
    int a[100], n, key, i;
    printf("Enter number of elements: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    printf("Enter key: ");
    scanf("%d",&key);

    for(i=0;i<n;i++) {
        if(a[i]==key) {
            printf("Found at position %d", i+1);
            return 0;
        }
    }
    printf("Not Found");
    return 0;
}

SAMPLE OUTPUT
Enter number of elements: 5
10 20 30 40 50
Enter key: 30

Found at position 3
