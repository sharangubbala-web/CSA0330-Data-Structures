#include <stdio.h>

int main() {
    int n, a[100], i;
    printf("Enter number of elements: ");
    scanf("%d",&n);

    printf("Enter elements:\n");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    for(i=0;i<n;i++) {
        if(a[i]%2==0)
            printf("%d is Even\n",a[i]);
        else
            printf("%d is Odd\n",a[i]);
    }
    return 0;
}


SAMPLE OUTPUT : 
Enter number of elements: 3
Enter elements:
1 3 4 
1 is Odd
3 is Odd
4 is Even
