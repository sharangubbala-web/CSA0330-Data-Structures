#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *left, *right;
};

struct node* newNode(int x) {
    struct node* n = malloc(sizeof(struct node));
    n->data = x;
    n->left = n->right = NULL;
    return n;
}

void inorder(struct node* root) {
    if(root) {
        inorder(root->left);
        printf("%d ",root->data);
        inorder(root->right);
    }
}

int main() {
    struct node* root = newNode(1);
    root->left = newNode(2);
    root->right = newNode(3);

    inorder(root);
    return 0;
}


SAMPLE OUTPUT
2 1 3 
