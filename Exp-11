#include <stdio.h>
#define SIZE 5

int stack[SIZE], top = -1;

void push(int x) {
    if(top==SIZE-1)
        printf("Overflow\n");
    else
        stack[++top]=x;
}

void pop() {
    if(top==-1)
        printf("Underflow\n");
    else
        printf("Popped %d\n",stack[top--]);
}

void peek() {
    if(top!=-1)
        printf("Top = %d\n",stack[top]);
}

int main() {
    push(10);
    push(20);
    peek();
    pop();
    return 0;
}


SAMPLE OUTPUT 
Top = 20
Popped 20
