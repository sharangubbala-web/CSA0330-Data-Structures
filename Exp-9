#include <stdio.h>

int main() {
    int a[100], n, key, low=0, high, mid, i;

    printf("Enter number of elements (sorted): ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    printf("Enter key: ");
    scanf("%d",&key);

    high = n-1;
    while(low <= high) {
        mid = (low + high)/2;
        if(a[mid]==key) {
            printf("Found at position %d", mid+1);
            return 0;
        }
        else if(a[mid]<key)
            low = mid+1;
        else
            high = mid-1;
    }
    printf("Not Found");
    return 0;
}


SAMPLE OUTPUT
Enter number of elements (sorted): 3
10 20 30
Enter key: 20
Found at position 2
